<%= content_for(:title) do %>
  <%= "OpenLoge | Profil" || "OpenLoge"%>
<% end %>

<%= content_for(:description) do %>
  Services locaux durables
<% end %>

<div class="bg-visitor margined-navbar">
  <div class="filter-blue"></div>
  <div class="container">
    <% if @user.is_expert? %>
      <%= render 'show_expert_profile', expert: @user %>
    <% else %>
      <%= render 'show_member_profile', member: @user %>
    <% end %>
  </div>
</div>
